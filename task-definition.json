{
    "family": "stocky-rupturas",
    "executionRoleArn": "arn:aws:iam::381492238900:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "stocky-rupturas",
            "image": "381492238900.dkr.ecr.us-east-1.amazonaws.com/stocky-rupturas:latest",
            "cpu": 1024,
            "memoryReservation": 512,
            "portMappings": [
                {
                    "name": "stocky-rupturas-3000-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "healthCheck": {
                "command": [
                    "curl http://localhost:3000/import/health"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3
            },
            "systemControls": []
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "256",
    "memory": "512"
}